<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mungomash.workouthelper">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.IntervalWorksOut">
        <activity
            android:name="com.mungomash.workouthelper.MainActivity">
        </activity>
        <activity android:name="com.mungomash.workouthelper.WorkoutActivity" />
        <activity android:name="com.mungomash.workouthelper.WorkingOutActivity"/>
        <activity
            android:name="com.mungomash.workouthelper.LoginActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
    </application>

</manifest>